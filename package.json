{
  "name": "allin-webpack-boilerplate",
  "version": "1.0.0",
  "description": "allin boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "cross-env __IS_WEB__=1 webpack-dev-server --hot --config webpack.development.config.js",
    "build": "cross-env __IS_WEB__=1 webpack --config webpack.production.config.js",
    "build:page": "cross-env __IS_WEB__=1  webpack --config page.webpack.js",
    "build:service": "cross-env __IS_WEB__=1  webpack --config service.webpack.js",
    "build:service:na": "cross-env __IS_WEB__=1 __PLATFORM__=NA  webpack --config service.webpack.js&npm run cp:service",
    "build:common": "npm run build:page&&npm run build:service",
    "build:test": "cross-env __IS_WEB__=1 webpack --config webpack.production.test.config.js",
    "sync": "rsync -avP --exclude-from='rsync-exclude.list' /Users/fengc16/workspace/arlene/arleneH5/arlene-pfe ./",
    "cp:service": "cp -R ./node_modules/@arl/MPService/*.js /Users/fengc16/workspace/arlene/arleneiOS/ArleneiOS/assets/@arl/MPService"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/randoll127/allin-webpack-boilerplate.git"
